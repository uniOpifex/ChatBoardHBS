<div>
    <h1>{{board.name}}'s posts</h1>

   
    <div>
        <a href="/boards">Back to Boards</a>
    </div>
    
    {{#each board.posts}}
        <div>
            {{this.content}}
        </div>
    {{/each}}

     <div>
        <a href="/boards/{{board._id}}/posts/new">Create a New Post</a>
    </div>
    
</div>